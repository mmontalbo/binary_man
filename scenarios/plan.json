{
  "schema_version": 2,
  "binary": "ls",
  "default_env": {
    "LC_ALL": "C",
    "LANG": "C",
    "TZ": "UTC",
    "COLUMNS": "80"
  },
  "coverage": {
    "blocked": [
      {
        "item_ids": ["--time-style", "--full-time", "--time", "-t"],
        "reason": "timestamp volatility",
        "details": "needs deterministic mtimes/ctimes to validate output"
      },
      {
        "item_ids": ["--numeric-uid-gid", "-n", "-l", "-g", "-o", "--author"],
        "reason": "uid/gid variance",
        "details": "requires fixture ownership control"
      },
      {
        "item_ids": ["--inode", "-i", "--block-size", "--size", "-s"],
        "reason": "filesystem-dependent metadata",
        "details": "inode and block counts vary across filesystems"
      },
      {
        "item_ids": ["--context", "-Z"],
        "reason": "SELinux/xattrs",
        "details": "requires label preservation and host support"
      }
    ]
  },
  "scenarios": [
    {
      "id": "help",
      "kind": "help",
      "publish": true,
      "coverage_ignore": true,
      "argv": ["--help"],
      "timeout_seconds": 3,
      "net_mode": "off",
      "no_sandbox": false,
      "no_strace": true,
      "snippet_max_lines": 12,
      "snippet_max_bytes": 1024,
      "expect": { "exit_code": 0, "stdout_contains_all": ["Usage:"] }
    },
    {
      "id": "version",
      "publish": true,
      "argv": ["--version"],
      "timeout_seconds": 3,
      "net_mode": "off",
      "no_sandbox": false,
      "no_strace": true,
      "snippet_max_lines": 20,
      "snippet_max_bytes": 2048,
      "expect": {
        "exit_code": 0,
        "stdout_contains_all": ["GNU coreutils"],
        "stdout_regex_any": ["GNU coreutils"]
      }
    },
    {
      "id": "invalid-option",
      "publish": true,
      "coverage_ignore": true,
      "argv": ["--definitely-not-a-real-option"],
      "timeout_seconds": 3,
      "net_mode": "off",
      "no_sandbox": false,
      "no_strace": true,
      "snippet_max_lines": 20,
      "snippet_max_bytes": 2048,
      "expect": {
        "exit_code": 2,
        "stderr_contains_any": ["unrecognized option", "Try 'ls --help'"]
      }
    },
    {
      "id": "fixture-all",
      "publish": true,
      "coverage_tier": "behavior",
      "seed_dir": "fixtures/ls-basic",
      "cwd": ".",
      "argv": ["-a", "-1", "--color=never", "--hyperlink=never"],
      "timeout_seconds": 3,
      "net_mode": "off",
      "no_sandbox": false,
      "no_strace": true,
      "snippet_max_lines": 12,
      "snippet_max_bytes": 256,
      "expect": {
        "exit_code": 0,
        "stdout_regex_all": ["(?m)^\\.hidden$", "(?m)^alpha$", "(?m)^beta$"]
      }
    },
    {
      "id": "fixture-recursive",
      "publish": true,
      "coverage_tier": "behavior",
      "seed_dir": "fixtures/ls-basic",
      "cwd": ".",
      "argv": ["-R", "-1", "--color=never", "--hyperlink=never", "dir"],
      "timeout_seconds": 3,
      "net_mode": "off",
      "no_sandbox": false,
      "no_strace": true,
      "snippet_max_lines": 20,
      "snippet_max_bytes": 512,
      "expect": {
        "exit_code": 0,
        "stdout_regex_all": ["(?m)^dir:$", "(?m)^child$", "(?m)^dir/subdir:$", "(?m)^leaf$"]
      }
    },
    {
      "id": "fixture-classify",
      "publish": true,
      "coverage_tier": "behavior",
      "seed_dir": "fixtures/ls-basic",
      "cwd": ".",
