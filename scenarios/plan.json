      "timeout_seconds": 3,
      "net_mode": "off",
      "no_sandbox": false,
      "no_strace": true,
      "snippet_max_lines": 12,
      "snippet_max_bytes": 256,
      "expect": {
        "exit_code": 0,
        "stdout_regex_all": ["(?m)^dir/$", "(?m)^link_to_alpha@$", "(?m)^script\\*$"]
      }
    },
